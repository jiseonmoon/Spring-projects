<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.mapper.ReporterMapper">

	<insert id="insertReporter">
		INSERT INTO tbl_reporter(rid, ractivity)
		VALUES(#{rid},#{ractivity})
	</insert>

	<select id="selectReporterList"
		resultType="kr.co.domain.ReporterVO">
		SELECT * FROM tbl_reporter
	</select>

</mapper>